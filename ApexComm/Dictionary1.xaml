            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ListViewItem}">
                    <Grid/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="DataTemplate1">
        <Grid/>
    </DataTemplate>
    <GridView x:Key="ViewBase1"/>

</ResourceDictionary>